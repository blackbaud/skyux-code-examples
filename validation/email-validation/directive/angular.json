{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "validation-email-validation-directive": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/validation-email-validation-directive",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js", "@skyux/packages/polyfills"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [],
            "styles": ["src/styles.scss"],
            "scripts": [],
            "buildOptimizer": false,
            "optimization": false,
            "allowedCommonJsDependencies": ["fontfaceobserver", "@skyux/icons"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "validation-email-validation-directive:build"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "validation-email-validation-directive:build"
          }
        }
      }
    }
  }
}
